{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/movieTracker.exe",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
      "environment": [
        {
          "name": "PATH",
          "value": "C:\\MinGW\\bin;${env:PATH}"
        }
      ],
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
